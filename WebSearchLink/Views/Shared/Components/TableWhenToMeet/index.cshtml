@using WebSearchLink.Models.ScheduleModels
@model List<WhenToMeet>
@foreach (var item in Model)
{
    <tr>
        <td>@item.Id</td>
        <td class="fw-semibold text-dark">@item.Title</td>
        <td>@item.CreatedByAdmin.Email</td>
        <td>@item.CreatedAt</td>
        <td>0</td>
        <td>0</td>
        <td>
            <a data-id="@item.Id" asp-controller="WhenToMeet" asp-action="ReportChooseTime" asp-route-wtmId="@item.Id" class="btn btn-report-time-slot btn-sm btn-outline-primary">Báo cáo</a>
            <a data-id="@item.Id" class="btn btn-detail-time-slot btn-sm btn-outline-primary">Chi tiết</a>
            <a data-id="@item.Id" class="btn btn-sm btn-outline-danger ms-2">Xóa</a>
        </td>
    </tr>
}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">

<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>

<script src="~/asset/js/whentomeet/slot_detail.js" asp-append-version="true"> </script>